<?xml version="1.0"?>
<package>
  <name>adafruit_imu</name>
  <version>1.0.0</version>
  <description>
The Adafruit IMU package for duckietown. Reads sensor data from adafruit IMU and publishes it to `sensor_msgs.Imu` and sensor_msgs.MagneticField.
  </description>
  <maintainer email="mack@duckietown.org">Mack</maintainer>
  <author email="yershov@mit.edu">Dmitry Yershov</author>

  <license>GPLv3</license>

  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>duckietown_msgs</build_depend>
  <build_depend>roscpp</build_depend>
  <build_depend>rospy</build_depend>
  <build_depend>sensor_msgs</build_depend>
  <run_depend>duckietown_msgs</run_depend>
  <run_depend>roscpp</run_depend>
  <run_depend>rospy</run_depend>
  <run_depend>sensor_msgs</run_depend>

</package>
